<?xml version="1.0" encoding="UTF-8"?>
<set>
	<system>
		<proxyHost></proxyHost>
		<proxyPort></proxyPort>
	</system>
	<users>

	<user username="" password=""  stockPwd=""  host="s4.verycd.9wee.com" >
			<monstor caption="野训设置">
				<startTime caption="开始时间">1</startTime>
				<endTime caption="结束时间">8</endTime>
				<savePoint caption="保留精力点数">20</savePoint>
				<!-- 装备保留  3 蓝装以上 4 紫装以上 5 全卖 -->
				<quality>4</quality>
			</monstor>
			<warrior caption="大厅设置">
				<!-- 大厅方式选择 0 不挂 1 训练 2 授艺 -->
				<warriorType>1</warriorType>
				<!-- 是否需要辎重 0 不需要 1 需要 -->
				<needZiZhong>1</needZiZhong>
			</warrior>
			<duel caption="竞技场设置">
				<!--竞技开始时间-->
				<duelStart>12</duelStart>
				<!--竞技间隔 单位为分钟-->
				<duelDue>0</duelDue>
				<!--竞技卸武 1 卸武 0 不卸武-->
				<dropWeapon>0</dropWeapon>
			</duel>
			<task caption="日常任务设置">
				<!--收集类任务最小金额-->
				<minMoney>20000</minMoney>
				<!-- 天机堂修门选项 1 四海库房 2 万守关 3 玲珑阁 4 藏书馆 0 不修 -->
				<tianJiDoor>1</tianJiDoor>
<!-- 购买种子,参考种子.txt 填写种子编码 ,不购买则不填写-->
				<buySeed></buySeed>
				<!-- 是否自动收获 1 是，0 否-->
				<autoHarvest>1</autoHarvest>
				<!-- 种植时间，逗号分割。1=星期天,2=星期一,3=星期二,4=星期三,5=星期四,6=星期五,7=星期六。 对应每周7天 ，周三会被系统清理，建议周3不种植-->
				<plantDay>1,4,5,6,7</plantDay>
			</task>
			<slavy caption="奴隶设置">
				<!-- 是否抓奴 0 不需要 1需要 -->
				<catchSlavy>1</catchSlavy>
				<!-- 最少奴隶个数 -->
				<minSlavy>3</minSlavy>
				<!-- 折磨方式 1 宣传 2 修门 -->
				<painType>2</painType>
				<!-- 修门选项 2 玄武 3 朱雀 3 白虎 4 青龙-->
				<build>2</build>
				<!-- 黑屋开始时间 -->
				<blackStart>0</blackStart>
				<!-- 黑屋结束时间 -->
				<blackEnd>0</blackEnd>
			</slavy>
			<daily caption="日常设置">
				<!-- 换荣誉 0 不换 1 换经验 2换紫玉 3 祖母绿-->
				<buyGlory>2</buyGlory>
				<!-- 是否需要领取每日福利礼包 1 领取 0不领 -->
				<dailyWeal>1</dailyWeal>
				<!-- 是否报名国都演武 1 报名 0 不报 -->
				<guoDuYanWu>0</guoDuYanWu>
				<!-- 渑池演武厅是否卸武 1 卸武 2 不卸武 -->
				<mianChi>2</mianChi>
				<needReadSave>0</needReadSave>

			</daily>
			<team caption="踢馆设置">
				<!-- 是否需要护联盟馆 -->
				<needProtect>1</needProtect>
				<!-- 护馆选项：低于多少强制护馆  -->
				<protectMyTeam>0.5</protectMyTeam>
				<!-- 是否需要踢馆 -->
				<needDesdroy>1</needDesdroy>
				<!-- 踢馆名称  设置了就不随时扔砖了哦-->
				<teamName></teamName>
			</team>
			<soul caption="武魂设置">
				<!-- 是否需要训练 0 否 1 是 -->
				<needTrain>0</needTrain>
				<!-- 是否需要提升品质，只用铜板 0 否 1 是 -->
				<needProm>1</needProm>
				<longStart>23</longStart>
				<!-- 1=铜板 2=1礼券/黄金 3=3礼券/黄金 4=8礼券/黄金 5=12礼券/黄金 -->
				<promType>1</promType>	
				<!-- 0=黄金 1=礼券 -->
				<promUse>1</promUse>
			</soul>
		</user>
		
	</users>
</set>